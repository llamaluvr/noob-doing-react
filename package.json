{
  "name": "codemash-js-dev-env",
  "version": "1.0.0",
  "description": "JS Dev Environment",
  "scripts": {
    "test": "jest --watch",
    "build": "webpack"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.16.0",
    "eslint": "3.12.2",
    "eslint-loader": "1.6.1",
    "express": "4.14.0",
    "jest": "18.0.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "no-debugger": "warn"
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.1"
  }
}
